// 키 바인딩을 이 파일에 넣어서 기본값 재정의
[
  {
    "key": "cmd+k",
    "command": "-git.commitAll",
    "when": "!inDebugMode && !operationInProgress && !terminalFocus"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "cmd+f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && isWeb"
  },
  {
    "key": "ctrl+shift+b",
    "command": "-editor.action.goToTypeDefinition",
    "when": "editorHasTypeDefinitionProvider && editorTextFocus"
  },
  {
    "key": "f12",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "alt+f12",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active && terminalFocus"
  },
  {
    "key": "alt+f12",
    "command": "-workbench.action.terminal.focus",
    "when": "terminalHasBeenCreated && !terminalFocus || terminalProcessSupported && !terminalFocus"
  },
  {
    "key": "alt+cmd+k",
    "command": "-git.pushTo",
    "when": "!inDebugMode && !operationInProgress && !terminalFocus"
  },
  {
    "key": "cmd+t",
    "command": "-git.sync",
    "when": "!operationInProgress"
  },
  {
    "key": "alt+cmd+z",
    "command": "-git.revertSelectedRanges",
    "when": "editorTextFocus && !editorReadonly && !operationInProgress"
  },
  {
    "key": "alt+cmd+z",
    "command": "git.clean"
  },
  {
    "key": "ctrl+alt+cmd+right",
    "command": "-quickInput.acceptInBackground",
    "when": "cursorAtEndOfQuickInputBox && inQuickInput && quickInputType == 'quickPick' || inQuickInput && !inputFocus && quickInputType == 'quickPick'"
  },
  {
    "key": "ctrl+alt+cmd+up",
    "command": "-quickInput.previousSeparator",
    "when": "inQuickInput && quickInputType == 'quickPick'"
  },
  {
    "key": "ctrl+alt+cmd+down",
    "command": "-quickInput.nextSeparator",
    "when": "inQuickInput && quickInputType == 'quickPick'"
  },
  {
    "key": "j",
    "command": "-list.focusDown",
    "when": "notebookEditorFocused && !inputFocus && !notebookOutputFocused"
  },
  {
    "key": "k",
    "command": "-list.focusUp",
    "when": "notebookEditorFocused && !inputFocus && !notebookOutputFocused"
  },
  {
    "key": "k",
    "command": "list.focusUp",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "j",
    "command": "list.focusDown",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "enter",
    "command": "-renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "left",
    "command": "-list.collapse",
    "when": "listFocus && treeElementCanCollapse && !inputFocus && !treestickyScrollFocused || listFocus && treeElementHasParent && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "right",
    "command": "-list.expand",
    "when": "listFocus && treeElementCanExpand && !inputFocus && !treestickyScrollFocused || listFocus && treeElementHasChild && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "cmd+o",
    "command": "-workbench.action.showAllSymbols"
  },
  {
    "key": "ctrl+cmd+up",
    "command": "breadcrumbs.focusAndSelect",
    "when": "breadcrumbsPossible && breadcrumbsVisible"
  },
  {
    "key": "cmd+up",
    "command": "-breadcrumbs.focusAndSelect",
    "when": "breadcrumbsPossible && breadcrumbsVisible"
  },
  {
    "key": "cmd+down",
    "command": "-editor.action.goToDeclaration",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+r cmd+s",
    "command": "workbench.action.openGlobalKeybindings"
  },
  {
    "key": "cmd+r cmd+s",
    "command": "-workbench.action.openGlobalKeybindings"
  },
  {
    "key": "cmd+f18",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "!editorFocus && terminalFocus"
  },
  {
    "key": "cmd+f18",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "editorFocus && !terminalFocus"
  },
  {
    "key": "cmd+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "cmd+n",
    "command": "explorer.newFile",
    "when": "listFocus"
  },
  {
    "key": "cmd+d",
    "command": "explorer.newFolder",
    "when": "listFocus"
  },
  {
    "key": "ctrl+alt+cmd+h",
    "command": "timeline.focus"
  },
  {
    "key": "ctrl+;",
    "command": "metaGo.gotoSmart"
  },
  {
    "key": "f7",
    "command": "references-view.findReferences",
    "when": "editorHasReferenceProvider"
  },
  {
    "key": "alt+f7",
    "command": "-references-view.findReferences",
    "when": "editorHasReferenceProvider"
  },
  {
    "key": "alt+cmd+j",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "alt+cmd+k",
    "command": "editor.emmet.action.removeTag",
